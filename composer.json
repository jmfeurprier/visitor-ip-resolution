{
	"name"        : "jmf/visitor-ip-resolution",
	"type"        : "library",
	"license"     : "MIT",
	"description" : "Allows resolving current visitor IP address.",
	"require"     : {
		"php" : ">=8.3",
		"webmozart/assert": "^1.11"
	},
	"require-dev": {
		"ext-xdebug"                : "*",
		"phing/phing"               : "^2.16",
		"phpmd/phpmd"               : "^2.8",
		"phpunit/phpunit"           : "^11.1",
		"squizlabs/php_codesniffer" : "^3.5",
		"phpstan/phpstan": "^1.10",
		"rector/rector": "^1.0"
	},
	"autoload" : {
		"psr-4" : {
			"Jmf\\VisitorIpResolution\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Jmf\\VisitorIpResolution\\" : "tests/unit/src/"
		}
	}
}
